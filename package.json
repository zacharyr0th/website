{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.15.4",
  "sideEffects": false,
  "engines": {
    "node": "^20.11.1",
    "pnpm": ">=8.15.4"
  },
  "files": [
    "app/**/*",
    "public/**/*",
    "!public/audio/**/*",
    "!**/coverage/**",
    "!**/.env*",
    "!**/*.log",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "NODE_ENV=production ANALYZE=false next build --no-lint",
    "start": "next start",
    "lint": "eslint . --config .eslintrc.cjs",
    "lint:fix": "eslint . --config .eslintrc.cjs --fix",
    "format": "prettier --write .",
    "check": "pnpm lint:fix && pnpm format && pnpm build",
    "clean": "rm -rf .next out node_modules/.cache coverage .turbo pnpm-lock.yaml node_modules"
  },
  "dependencies": {
    "@aws-sdk/s3-request-presigner": "3.777.0",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/bundle-analyzer": "^15.3.0",
    "@next/env": "^15.3.0",
    "@next/mdx": "^15.3.0",
    "@next/swc-darwin-arm64": "^15.3.0",
    "@types/lodash": "^4.17.16",
    "@vercel/analytics": "^1.4.1",
    "caniuse-lite": "^1.0.30001695",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "framer-motion": "^12.6.5",
    "geist": "^1.3.1",
    "gray-matter": "^4.0.3",
    "isomorphic-dompurify": "^2.21.0",
    "lodash": "^4.17.21",
    "lru-cache": "11.0.2",
    "lucide-react": "^0.487.0",
    "marked": "^15.0.8",
    "next": "^15.3.0",
    "next-mdx-remote": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^5.0.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "sharp": "^0.33.5",
    "styled-jsx": "^5.1.6",
    "tailwind-merge": "^3.2.0",
    "unified": "^11.0.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "3.749.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/crypto-js": "^4.2.1",
    "@types/lru-cache": "7.10.9",
    "@types/node": "^20.17.30",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@types/tailwindcss": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.20",
    "critters": "^0.0.23",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.3.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-security": "^2.1.1",
    "mime-types": "2.1.35",
    "postcss": "^8.5.1",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.4.2",
    "snyk": "^1.1247.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.3",
    "vercel": "^41.6.0"
  }
}
