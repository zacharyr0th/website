{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.2.4",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "dev:clean": "rm -rf .next && next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:prod": "npm prune --production && next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "type-check": "tsc --noEmit",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "check": "npm run type-check && npm run lint:fix && npm run format",
    "clean": "rm -rf .next out",
    "clean:deps": "rm -rf node_modules",
    "clean:all": "npm run clean && npm run clean:deps && rm -rf dist package-lock.json",
    "update": "npm update",
    "update:check": "npm outdated",
    "deps:clean": "npm prune",
    "reset:quick": "npm run clean && npm run dev",
    "reset:deps": "npm run clean:deps && npm install && npm run dev",
    "reset:hard": "npm run clean:all && npm install && npm run check && npm run build",
    "reset:cache": "npm cache verify && npm cache clean --force || true",
    "reset": "npm run clean:all && npm cache clean --force && npm prune && npm update --save && npm install && npm run type-check && npm run lint:fix && npm run format && npm run test:ci && npm run build"
  },
  "dependencies": {
    "@vercel/analytics": "^1.4.1",
    "caniuse-lite": "^1.0.30001690",
    "clsx": "^2.1.1",
    "framer-motion": "^10.18.0",
    "gray-matter": "^4.0.3",
    "next": "^15.1.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^5.0.0",
    "react-icons": "^4.12.0",
    "react-swipeable": "^7.0.2",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-html": "^16.0.1",
    "remark-images": "^4.1.0",
    "remark-parse": "^11.0.0",
    "styled-jsx": "^5.1.6",
    "swr": "^2.3.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.23",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.12",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.20",
    "critters": "^0.0.23",
    "eslint": "^8.57.1",
    "eslint-config-next": "13.5.6",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.7.2"
  }
}
