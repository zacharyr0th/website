## Required

#  The public URL of your site
NEXT_PUBLIC_SITE_URL=http://localhost:3000


## Optional

# Google Analytics tracking ID
NEXT_PUBLIC_GA_ID=
# Google site verification code
NEXT_PUBLIC_GOOGLE_SITE_VERIFICATION=
# Maximum file size for articles in bytes (default: 5MB)
NEXT_PUBLIC_MAX_ARTICLE_SIZE=5242880
# Cache duration for static pages in seconds (default: 3600)
NEXT_PUBLIC_CACHE_DURATION=3600
# API rate limiting (requests per window)
NEXT_PUBLIC_RATE_LIMIT=100
# Rate limit window in milliseconds
NEXT_PUBLIC_RATE_LIMIT_WINDOW=900000 